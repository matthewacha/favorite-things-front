{
  "name": "favorite-things-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm install -g serve && serve -s dist",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "mocha-webpack -w --webpack-config webpack.config.js tests/**/*.spec.js",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:je": "jest --config jest.config.js -u --coverage",
    "watch": "webpack -w -d",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text yarn test:unit"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-rc.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "esm": "^3.2.25",
    "expect": "^24.8.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "mocha-webpack": "^2.0.0-beta.0",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-notification": "^1.3.16",
    "vue-router": "^3.0.3",
    "vue-svg-inline-loader": "^1.2.16",
    "vuetify": "^1.5.16",
    "vuetify-loader": "^1.2.2",
    "vuex": "^3.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-cypress": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-plugin-unit-mocha": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.28",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": [
      "src/**/*.vue",
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false,
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "coverage"
    ]
  }
}
